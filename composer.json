{
    "name": "quartet/webpay-bundle",
    "description": "Integration of webpay into symfony2.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Shota Hoshino",
            "email": "lga0503@gmail.com"
        }
    ],
    "require": {
        "PHP": ">=5.4",
        "symfony/framework-bundle": "~2.5",
        "webpay/webpay": "~1.1",
        "twig/twig": "~1.15"
    },
    "require-dev": {
        "symfony/browser-kit": "~2.5",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Quartet\\Bundle\\WebPayBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Quartet\\Bundle\\WebPayBundle\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
